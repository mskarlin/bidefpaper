@article{Zeller2006,
abstract = {The main aim of this presentation is to give an introduction into$\backslash$nspin-polarized density- functional-theory (DFT) calculations as a$\backslash$nmethod to predict and understand the band magnetism of itinerant$\backslash$nelectrons at zero temperature. It will be pointed out that, for instance$\backslash$nthe ferromagnetism of Fe, Co, and Ni is correctly predicted by such$\backslash$ncalculations without adjustable parameters and that the results can$\backslash$nbe explained in terms of a simple Stoner model. For practical applications$\backslash$nit will be discussed why the convergence of density-functional self-consistency$\backslash$niterations can be very slow for magnetic systems and how this problem$\backslash$ncan be alleviated. As illustrating examples of recent applications$\backslash$nin computational nanoscience, results for magnetic monatomic rows$\backslash$non noble metal substrates will be presented. Finally it will be argued$\backslash$nthat the magnetism in certain materials, for instance in antiferromagnets,$\backslash$ndoes not arise from rigid-band shifts as in the Stoner model, but$\backslash$nfrom covalent interactions between the electronic states of the participating$\backslash$natoms.},
author = {Zeller, Rudolf},
file = {:home/qgroup/Journal Papers/DFT/zeller.pdf:pdf},
isbn = {3000173501},
journal = {Computational Nanoscience: DDo It Yourself!},
pages = {419--445},
