@article{Pavese1999,
abstract = {A fully quantum molecular dynamics method is presented which combines$\backslash$nab initio Car-Parrinello molecular dynamics with centroid molecular$\backslash$ndynamics. The first technique allows the forces on the atoms to be$\backslash$nobtained from ab initio electronic structure. The second technique,$\backslash$ngiven the forces on the atoms, allows one to calculate an approximate$\backslash$nquantum time evolution for the nuclei. The combination of the two,$\backslash$ntherefore, represents the first feasible approach to simulating the$\backslash$nfully quantum dynamics of a many-body system. An application to excess$\backslash$nproton translocation along a model water wire will be presented.$\backslash$n(C) 1999 Elsevier Science B.V. All rights reserved.},
