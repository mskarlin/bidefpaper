journal = {The Journal of Chemical Physics},
keywords = {Fermi level,adsorbed layers,density functional theory,integral equations,interface states,liquid theory,metals,water},
number = {20},
pages = {10095--10112},
publisher = {Aip},
title = {{Self-consistent description of a metal--water interface by the Kohn--Sham density functional theory and the three-dimensional reference interaction site model}},
