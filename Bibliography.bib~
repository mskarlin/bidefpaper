journal = {Nature nanotechnology},
keywords = {Copper,Copper: chemistry,Materials Testing,Metal Nanoparticles,Metal Nanoparticles: chemistry,Microscopy, Electron, Transmission,Models, Molecular,Molecular Dynamics Simulation,Particle Size,Tensile Strength},
month = sep,
