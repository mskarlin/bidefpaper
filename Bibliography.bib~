journal = {Nature},
month = jan,
number = {7075},
pages = {419--25},
pmid = {16437105},
title = {{Atomic packing and short-to-medium-range order in metallic glasses.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/16437105},
volume = {439},
year = {2006}
