@article{Cococcioni2005,
abstract = {In this work we reexamine the LDA + U method of Anisimov and co-workers$\backslash$nin the framework of a plane-wave pseudopotential approach. A simplified$\backslash$nrotational-invariant formulation is adopted. The calculation of the$\backslash$nHubbard U entering the expression of the functional is discussed$\backslash$nand a linear response approach is proposed that is internally consistent$\backslash$nwith the chosen definition for the occupation matrix of the relevant$\backslash$nlocalized orbitals. In this way we obtain a scheme whose functionality$\backslash$nshould not depend strongly on the particular implementation of the$\backslash$nmodel in ab initio calculations. We demonstrate the accuracy of the$\backslash$nmethod, computing structural and electronic properties of a few systems$\backslash$nincluding transition and rare-earth correlated metals, transition$\backslash$nmetal monoxides, and iron silicate.},
