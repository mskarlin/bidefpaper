issn = {1931-7573},
journal = {Nanoscale research letters},
keywords = {microscope,modulus \'{a},nanoindentation \'{a} atomic force,park equally contributed to,silicon nanowire \'{a} elastic,this work,young-soo sohn and jinsung},
