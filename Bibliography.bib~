@article{Wang2006,
abstract = {The energy of a large number of oxidation reactions of 3d transition$\backslash$nmetal oxides is computed using the generalized gradient approach$\backslash$n(GGA) and GGA+U methods. Two substantial contributions to the error$\backslash$nin GGA oxidation energies are identified. The first contribution$\backslash$noriginates from the overbinding of GGA in the O2 molecule and only$\backslash$noccurs when the oxidant is O2. The second error occurs in all oxidation$\backslash$nreactions and is related to the correlation error in 3d orbitals$\backslash$nin GGA. Strong self-interaction in GGA systematically penalizes a$\backslash$nreduced state (with more d electrons) over an oxidized state, resulting$\backslash$nin an overestimation of oxidation energies. The constant error in$\backslash$nthe oxidation energy from the O2 binding error can be corrected by$\backslash$nfitting the formation enthalpy of simple nontransition metal oxides.$\backslash$nRemoval of the O2 binding error makes it possible to address the$\backslash$ncorrelation effects in 3d transition metal oxides with the GGA+U$\backslash$nmethod. Calculated oxidation energies agree well with experimental$\backslash$ndata for reasonable and consistent values of U.},
author = {Wang, Lei and Maxisch, Thomas and Ceder, Gerbrand},
doi = {10.1103/PhysRevB.73.195107},
