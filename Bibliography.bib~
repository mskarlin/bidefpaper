@article{Argaman,
archivePrefix = {arXiv},
arxivId = {arXiv:physics/9806013v2},
author = {Argaman, Nathan and Makov, Guy},
eprint = {9806013v2},
file = {:home/qgroup/Journal Papers/DFT/Alternative formulation of DFT:},
primaryClass = {arXiv:physics},
title = {{Density Functional Theory — an introduction}}
}
@article{Argaman1998,
abstract = {Density Functional Theory (DFT) is one of the most widely used methods for "ab initio" calculations of the structure of atoms, molecules, crystals, surfaces, and their interactions. Unfortunately, the customary introduction to DFT is often considered too lengthy to be included in various curricula. An alternative introduction to DFT is presented here, drawing on ideas which are well-known from thermodynamics, especially the idea of switching between different independent variables. The central theme of DFT, i.e. the notion that it is possible and beneficial to replace the dependence on the external potential v(r) by a dependence on the density distribution n(r), is presented as a straightforward generalization of the familiar Legendre transform from the chemical potential ($\backslash$mu) to the number of particles (N). This approach is used here to introduce the Hohenberg-Kohn energy functional and to obtain the corresponding theorems, using classical nonuniform fluids as simple examples. The energy functional for electronic systems is considered next, and the Kohn-Sham equations are derived. The exchange-correlation part of this functional is discussed, including both the local density approximation to it, and its formally exact expression in terms of the exchange-correlation hole. A very brief survey of various applications and extensions is included.},
archivePrefix = {arXiv},
arxivId = {physics/9806013},
author = {Argaman, Nathan and Makov, Guy},
doi = {10.1119/1.19375},
eprint = {9806013},
file = {:home/qgroup/Journal Papers/DFT/Alternative formulation of DFT:},
month = jun,
primaryClass = {physics},
title = {{Density Functional Theory -- an introduction}},
url = {http://arxiv.org/abs/physics/9806013},
year = {1998}
}
